{
  StringBuilder sb=new StringBuilder();
  sb.append("{");
  sb.append("\"hiddenFields\":[");
  for (int j=0; j < hiddenFields.size(); j++) {
    sb.append("{\"name\":\"");
    sb.append(hiddenFields.get(j).getName());
    sb.append("\",\"val\":\"");
    sb.append(hiddenFields.get(j).getValue());
    sb.append("\"}");
    if (j < hiddenFields.size() - 1) {
      sb.append(",");
    }
  }
  sb.append("]}");
  return sb.toString();
}
