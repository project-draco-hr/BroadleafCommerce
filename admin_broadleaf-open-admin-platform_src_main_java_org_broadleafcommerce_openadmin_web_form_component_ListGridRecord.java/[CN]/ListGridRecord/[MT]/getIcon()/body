{
  if (icon != null) {
    return icon;
  }
  if (getIsError()) {
    return new ListGridRecordIcon().withCssClass("icon-exclamation-sign").withMessage(BLCMessageUtils.getMessage(getErrorKey()));
  }
  if (getIsDirty()) {
    return new ListGridRecordIcon().withCssClass("icon-pencil").withMessage(BLCMessageUtils.getMessage("listgrid.record.edited"));
  }
  return null;
}
