{
  if (this == o) {
    return true;
  }
  if (!getClass().isAssignableFrom(o.getClass())) {
    return false;
  }
  Money money=(Money)o;
  if (amount != null ? !amount.equals(money.amount) : money.amount != null) {
    return false;
  }
  if (isZero()) {
    return true;
  }
  if (currency != null ? !currency.equals(money.currency) : money.currency != null) {
    return false;
  }
  return true;
}
