{
  List<SearchFacetDTO> facets=new ArrayList<SearchFacetDTO>();
  for (  SearchFacet facet : categoryFacets) {
    SearchFacetDTO dto=new SearchFacetDTO();
    dto.setFacet(facet);
    dto.setShowQuantity(false);
    dto.setFacetValues(getFacetValues(facet));
    dto.setActive(true);
    facets.add(dto);
  }
  return facets;
}
