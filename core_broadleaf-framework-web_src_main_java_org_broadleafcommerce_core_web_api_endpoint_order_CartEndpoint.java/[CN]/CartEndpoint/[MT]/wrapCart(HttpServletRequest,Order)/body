{
  try {
    OrderWrapper orderWrapper=(OrderWrapper)context.getBean(OrderWrapper.class.getName());
    orderWrapper.wrapDetails(cart,request);
    return orderWrapper;
  }
 catch (  BeansException e) {
    throw new WebApplicationException(Response.status(Response.Status.INTERNAL_SERVER_ERROR).type(MediaType.TEXT_PLAIN).entity("Problem displaying cart").build());
  }
}
