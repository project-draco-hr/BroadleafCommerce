{
  if (ArrayUtils.isEmpty(customCriteria)) {
    return -1;
  }
  for (int i=0; i < customCriteria.length; i++) {
    if (customCriteria[i] != null && customCriteria[i].equals(criteria)) {
      return i;
    }
 else     if (customCriteria[i] == null && criteria == null) {
      return i;
    }
  }
  return -1;
}
