{
  if (field.getFacetFieldType() == null) {
    return null;
  }
  return new StringBuilder().append(getGlobalPrefix()).append(field.getFacetFieldType().equals(FieldType.PRICE) ? getPricelistPrefix() : getLocalePrefix()).append(field.getPropertyName()).append("_").append(field.getFacetFieldType().getType()).toString();
}
