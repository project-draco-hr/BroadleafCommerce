{
  List<Product> featuredProducts=new ArrayList<Product>();
  if (products != null) {
    Iterator<Product> i=products.iterator();
    while (i.hasNext()) {
      Product p=i.next();
      if (p.getIsFeaturedProduct()) {
        featuredProducts.add(p);
      }
    }
  }
  return featuredProducts;
}
