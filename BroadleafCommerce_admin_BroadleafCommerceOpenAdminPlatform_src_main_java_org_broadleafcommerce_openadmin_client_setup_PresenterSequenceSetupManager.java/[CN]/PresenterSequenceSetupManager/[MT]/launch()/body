{
  if (!BLCMain.ISNEW) {
    BLCMain.MODAL_PROGRESS.startProgress(new Timer(){
      public void run(){
        if (!presenter.getLoaded()) {
          itemsIterator=items.iterator();
          next();
        }
      }
    }
);
  }
 else {
    if (!presenter.getLoaded()) {
      itemsIterator=items.iterator();
      next();
    }
  }
}
