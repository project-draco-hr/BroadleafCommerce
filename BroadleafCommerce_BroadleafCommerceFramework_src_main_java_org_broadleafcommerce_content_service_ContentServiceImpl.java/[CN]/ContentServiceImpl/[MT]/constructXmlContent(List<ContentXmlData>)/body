{
  if (details != null) {
    String xml="";
    xml+="<?xml version=\"1.0\" encoding=\"UTF-8\"?><content>";
    for (Iterator<ContentXmlData> itr=details.iterator(); itr.hasNext(); ) {
      ContentXmlData data=itr.next();
      xml+="<" + data.getName() + ">";
      xml+="<![CDATA[";
      xml+=data.getData().toString();
      xml+="]]>";
      xml+="</" + data.getName() + ">";
    }
    xml+="</content>";
    return xml;
  }
  return null;
}
