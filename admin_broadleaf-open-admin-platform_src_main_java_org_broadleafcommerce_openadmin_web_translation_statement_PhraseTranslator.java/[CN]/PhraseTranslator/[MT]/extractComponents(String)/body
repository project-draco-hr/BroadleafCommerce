{
  String[] components=new String[]{};
  for (  String operator : STANDARDOPERATORS) {
    String[] temp=phrase.split(operator);
    if (temp.length == 2) {
      components=new String[3];
      components[0]=temp[0];
      components[1]=operator;
      components[2]=temp[1];
      break;
    }
    components=temp;
  }
  if (components.length != 3) {
    try {
      for (      String key : SPECIALCASES) {
        if (components[0].indexOf(key) >= 0) {
          String[] temp=extractSpecialComponents(components,key);
          components=temp;
          break;
        }
      }
    }
 catch (    Exception e) {
    }
    if (components.length != 3) {
      try {
        String[] temp=extractProjection(components);
        components=temp;
      }
 catch (      Exception e1) {
      }
      if (components.length != 3) {
        throw new MVELTranslationException("Could not parse the MVEL expression to a " + "compatible form for the rules builder (" + phrase + ")");
      }
    }
  }
  return components;
}
