{
  BandedPriceFulfillmentOption option=createBands(new String[]{"10","10","10"},new String[]{"30","20","10"},new FulfillmentBandResultAmountType[]{FulfillmentBandResultAmountType.RATE,FulfillmentBandResultAmountType.RATE,FulfillmentBandResultAmountType.RATE});
  assertEquals(calculationResponse(option,createCandidateOrder(new BigDecimal("10.00"),2)),new Money("10.00"));
}
