{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  CustomerAddressImpl other=(CustomerAddressImpl)obj;
  if (address == null) {
    if (other.address != null)     return false;
  }
 else   if (!address.equals(other.address))   return false;
  if (addressName == null) {
    if (other.addressName != null)     return false;
  }
 else   if (!addressName.equals(other.addressName))   return false;
  if (customerId == null) {
    if (other.customerId != null)     return false;
  }
 else   if (!customerId.equals(other.customerId))   return false;
  return true;
}
