{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  Property other=(Property)obj;
  if (metadata == null || metadata.getMergedPropertyType() == null) {
    if (other.metadata != null && other.metadata.getMergedPropertyType() != null)     return false;
  }
 else   if (!metadata.getMergedPropertyType().equals(other.metadata.getMergedPropertyType()))   return false;
  if (name == null) {
    if (other.name != null)     return false;
  }
 else   if (!name.equals(other.name))   return false;
  return true;
}
