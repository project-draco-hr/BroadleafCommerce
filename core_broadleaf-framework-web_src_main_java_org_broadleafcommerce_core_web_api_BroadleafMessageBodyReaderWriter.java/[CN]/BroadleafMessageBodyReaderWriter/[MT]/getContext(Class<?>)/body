{
  if (jaxbContext == null) {
synchronized (this) {
      initializeTypeMap();
      if (!typeMap.isEmpty()) {
        try {
          jaxbContext=JAXBContext.newInstance(typeMap.values().toArray(new Class<?>[typeMap.size()]));
        }
 catch (        JAXBException e) {
          throw new RuntimeException("Error creating a JAXBContext",e);
        }
      }
    }
  }
  return jaxbContext;
}
