{
  clearCache();
  Object returnValue;
  try {
    returnValue=call.proceed();
  }
  finally {
    Order orderItem=getLatestItem();
    clearCache();
    if (orderItem != null) {
      orderDao.save(orderItem);
      orderItem=pricingService.executePricing(orderItem);
      orderDao.save(orderItem);
      LOG.debug("Context order priced : order id " + orderItem.getId());
    }
  }
  return returnValue;
}
