{
  if (o != null && getClass().isAssignableFrom(o.getClass())) {
    OfferImpl that=(OfferImpl)o;
    return new EqualsBuilder().append(this.id,that.id).append(this.name,that.name).append(this.startDate,that.startDate).append(this.type,that.type).append(this.value,that.value).build();
  }
  return false;
}
