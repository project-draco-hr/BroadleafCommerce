{
  if (item1.getSku() != null && item2.getSku() != null) {
    if (item1.getSku().getId().equals(item2.getSku().getId())) {
      return true;
    }
  }
 else {
    if (item1.getProduct() != null && item2.getProduct() != null) {
      if (item1.getProduct().getId().equals(item2.getProduct().getId())) {
        return true;
      }
    }
  }
  return false;
}
