{
  super();
  setWidth100();
  setBackgroundColor("#eaeaea");
  form=new DynamicForm();
  form.setHeight(175);
  form.setWidth100();
  form.setPadding(10);
  form.disable();
  form.setBackgroundColor("#eaeaea");
