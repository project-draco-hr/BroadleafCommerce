{
  if (!(seedData instanceof CheckoutSeed)) {
    throw new WorkflowException("Seed data instance is incorrect. " + "Required class is " + CheckoutSeed.class.getName() + " "+ "but found class: "+ seedData.getClass().getName());
  }
  OfferContext context=new OfferContext();
  context.setSeedData(((CheckoutSeed)seedData).getOrder());
  return context;
}
