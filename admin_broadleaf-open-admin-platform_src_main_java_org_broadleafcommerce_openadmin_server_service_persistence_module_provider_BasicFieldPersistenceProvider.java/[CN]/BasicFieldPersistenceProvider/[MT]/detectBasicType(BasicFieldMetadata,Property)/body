{
  return (metadata.getFieldType() == SupportedFieldType.BOOLEAN || metadata.getFieldType() == SupportedFieldType.DATE || metadata.getFieldType() == SupportedFieldType.DECIMAL || metadata.getFieldType() == SupportedFieldType.INTEGER || metadata.getFieldType() == SupportedFieldType.EMAIL || metadata.getFieldType() == SupportedFieldType.FOREIGN_KEY || metadata.getFieldType() == SupportedFieldType.ADDITIONAL_FOREIGN_KEY || metadata.getFieldType() == SupportedFieldType.STRING || metadata.getFieldType() == SupportedFieldType.ID) && (property == null || !property.getName().contains(FieldManager.MAPFIELDSEPARATOR));
}
