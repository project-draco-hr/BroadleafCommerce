{
  Collections.sort(fieldPersistenceProviders,new Comparator<FieldPersistenceProvider>(){
    @Override public int compare(    FieldPersistenceProvider o1,    FieldPersistenceProvider o2){
      return Integer.compare(o1.getOrder(),o2.getOrder());
    }
  }
);
  Collections.sort(populateValidators,new Comparator<PopulateValueRequestValidator>(){
    @Override public int compare(    PopulateValueRequestValidator o1,    PopulateValueRequestValidator o2){
      return Integer.compare(o1.getOrder(),o2.getOrder());
    }
  }
);
}
