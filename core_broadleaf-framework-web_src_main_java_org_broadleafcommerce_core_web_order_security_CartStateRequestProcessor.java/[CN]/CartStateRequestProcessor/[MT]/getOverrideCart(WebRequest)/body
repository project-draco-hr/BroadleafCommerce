{
  Long orderId=(Long)request.getAttribute(OVERRIDE_CART_ATTR_NAME,WebRequest.SCOPE_GLOBAL_SESSION);
  Order cart=null;
  if (orderId != null) {
    cart=orderService.findOrderById(orderId);
    if (cart == null || cart.getStatus().equals(OrderStatus.SUBMITTED) || cart.getStatus().equals(OrderStatus.CANCELLED)) {
      return null;
    }
  }
  return cart;
}
