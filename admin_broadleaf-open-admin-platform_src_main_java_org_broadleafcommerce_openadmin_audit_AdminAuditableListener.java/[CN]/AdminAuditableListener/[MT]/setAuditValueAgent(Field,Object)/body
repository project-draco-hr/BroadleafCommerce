{
  try {
    AdminSandBoxContext context=(AdminSandBoxContext)SandBoxContext.getSandBoxContext();
    if (context != null) {
      field.setAccessible(true);
      field.set(entity,context.getAdminUser());
    }
  }
 catch (  IllegalStateException e) {
