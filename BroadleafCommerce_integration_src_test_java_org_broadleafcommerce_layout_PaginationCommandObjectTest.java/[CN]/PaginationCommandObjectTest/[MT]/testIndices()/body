{
  PaginationCommandObject pco=new PaginationCommandObject();
  pco.setPageNumber(1);
  pco.setPageSize(25);
  pco.setListSize(60);
  assert(pco.getStartIndex() == 25);
  assert(pco.getEndIndex() == 50);
  pco.setPageNumber(2);
  assert(pco.getStartIndex() == 50);
  assert(pco.getEndIndex() == 60);
}
