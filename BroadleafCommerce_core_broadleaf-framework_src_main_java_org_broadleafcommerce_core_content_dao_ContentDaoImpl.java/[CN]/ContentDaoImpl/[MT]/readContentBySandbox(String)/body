{
  Query query=null;
  if (sandbox != null && sandbox.endsWith("*"))   query=em.createNamedQuery("BC_READ_CONTENT_BY_LIKE_SANDBOX");
 else   query=em.createNamedQuery("BC_READ_CONTENT_BY_SANDBOX");
  query.setParameter("sandbox",sandbox);
  query.setHint(QueryHints.HINT_CACHEABLE,true);
  return (List<Content>)query.getResultList();
}
