{
  if (skus == null) {
    skus=new ArrayList<Sku>();
    List<Sku> skus=getAllSkus();
    for (    Sku sku : skus) {
      if (sku.getActiveStartDate().before(new Date()) && (sku.getActiveEndDate() == null || new Date().before(sku.getActiveEndDate()))) {
        skus.add(sku);
      }
    }
  }
  return skus;
}
