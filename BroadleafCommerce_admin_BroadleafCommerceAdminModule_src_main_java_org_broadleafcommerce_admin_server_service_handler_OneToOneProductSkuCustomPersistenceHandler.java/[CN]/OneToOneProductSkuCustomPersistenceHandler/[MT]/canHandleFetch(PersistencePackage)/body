{
  String[] customCriteria=persistencePackage.getCustomCriteria();
  boolean canHandle=false;
  if (customCriteria != null) {
    for (    String criteria : customCriteria) {
      if (criteria != null && criteria.equals(IDENTITYCRITERIA)) {
        canHandle=true;
        break;
      }
    }
  }
  return canHandle;
}
