{
  SimpleRule simpleRule;
  if (!StringUtils.isEmpty(populateValueRequest.getMetadata().getToOneTargetProperty())) {
    simpleRule=(SimpleRule)populateValueRequest.getFieldManager().getFieldValue(parent,populateValueRequest.getMetadata().getToOneTargetProperty());
  }
 else {
    simpleRule=(SimpleRule)parent;
  }
  return simpleRule;
}
