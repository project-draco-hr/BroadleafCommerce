{
  operationTypes=new OperationTypes(OperationType.BASIC,OperationType.BASIC,OperationType.BASIC,OperationType.BASIC,OperationType.BASIC);
  PersistencePerspective persistencePerspective=new PersistencePerspective(operationTypes,new String[]{},new ForeignKey[]{});
  DataSourceModule[] modules=new DataSourceModule[]{new BasicClientEntityModule(CeilingEntities.PAGETEMPLATE,persistencePerspective,AppServices.DYNAMIC_ENTITY)};
  PageTemplateFormListDataSource dataSource=new PageTemplateFormListDataSource(name + count++,persistencePerspective,AppServices.DYNAMIC_ENTITY,modules);
  dataSource.buildFields((String[])additionalItems,false,cb);
}
