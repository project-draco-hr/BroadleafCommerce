{
  super();
  this.customer=customer;
  this.reivewSubmittedDate=reivewSubmittedDate;
  this.reviewText=reviewText;
  this.ratingSummary=ratingSummary;
  this.reviewFeedback=new ArrayList<ReviewFeedback>();
