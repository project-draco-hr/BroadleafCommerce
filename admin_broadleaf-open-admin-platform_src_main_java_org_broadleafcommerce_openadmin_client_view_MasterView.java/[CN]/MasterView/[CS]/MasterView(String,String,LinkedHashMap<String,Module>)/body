{
  MasterView.moduleKey=moduleKey;
  this.modules=modules;
  setWidth100();
  setHeight100();
  addMember(buildHeader());
  addMember(buildPrimaryMenu());
  addMember(buildSecondaryMenu(pageKey));
  canvas=new HLayout();
  canvas.setWidth100();
  canvas.setHeight100();
  addMember(canvas);
  buildFooter();
  bind();
}
