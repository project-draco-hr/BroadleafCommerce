{
  StringBuffer sb=new StringBuffer();
  boolean eof=false;
  BufferedReader reader=null;
  try {
    reader=new BufferedReader(new InputStreamReader(hostInput));
    while (!eof) {
      String temp=reader.readLine();
      if (temp == null) {
        eof=true;
      }
 else {
        sb.append(temp);
        sb.append("\n");
      }
    }
  }
  finally {
    if (reader != null) {
      try {
        reader.close();
      }
 catch (      Throwable e) {
      }
    }
  }
  return sb.toString();
}
