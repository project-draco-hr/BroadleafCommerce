{
  final int prime=31;
  int result=1;
  result=prime * result + ((id == null) ? 0 : id.hashCode());
  result=prime * result + ((name == null) ? 0 : name.hashCode());
  result=prime * result + ((product == null) ? 0 : product.hashCode());
  result=prime * result + ((value == null) ? 0 : value.hashCode());
  return result;
}
