{
  DynamicEntityFormInfo info=new DynamicEntityFormInfo().withCeilingClassName(StructuredContentType.class.getName()).withSecurityCeilingClassName(StructuredContent.class.getName()).withCriteriaName("constructForm").withPropertyName(propertyName).withPropertyValue(propertyTypeId);
  return super.getDynamicForm(request,response,model,pathVars,info);
}
