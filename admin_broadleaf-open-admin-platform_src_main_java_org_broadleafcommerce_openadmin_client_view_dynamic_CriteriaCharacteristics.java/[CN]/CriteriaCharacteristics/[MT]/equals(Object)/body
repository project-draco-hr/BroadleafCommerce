{
  if (this == o)   return true;
  if (!(o instanceof CriteriaCharacteristics))   return false;
  CriteriaCharacteristics that=(CriteriaCharacteristics)o;
  if (criteriaSize != that.criteriaSize)   return false;
  if (!operator.equals(that.operator) && criteriaSize != 0)   return false;
  return true;
}
