{
  Class<?> clazz=null;
  try {
    clazz=entityConfiguration.lookupEntityClass(className);
  }
 catch (  NoSuchBeanDefinitionException e) {
  }
  if (clazz == null) {
    try {
      clazz=Class.forName(className);
    }
 catch (    ClassNotFoundException e) {
      throw new RuntimeException(e);
    }
  }
  return clazz;
}
