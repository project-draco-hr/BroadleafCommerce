{
  tabName=tabName == null ? DEFAULT_TAB_NAME : tabName;
  tabOrder=tabOrder == null ? DEFAULT_TAB_ORDER : tabOrder;
  Tab tab=findTab(tabName);
  if (tab == null) {
    tab=new Tab();
    tab.setTitle(tabName);
    tab.setOrder(tabOrder);
    tabs.add(tab);
  }
  tab.getRuleBuilders().add(ruleBuilder);
}
