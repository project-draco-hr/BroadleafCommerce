{
  if (fields == null) {
    Map<String,Field> map=new HashMap<String,Field>();
    for (    Entry<String,FieldGroup> entry : groups.entrySet()) {
      for (      Field field : entry.getValue().getFields()) {
        map.put(field.getName(),field);
      }
    }
    fields=map;
  }
  return fields;
}
