{
  if (fields == null) {
    Map<String,Field> map=new LinkedHashMap<String,Field>();
    for (    Tab tab : tabs) {
      for (      FieldGroup group : tab.getFieldGroups()) {
        for (        Field field : group.getFields()) {
          map.put(field.getName(),field);
        }
      }
    }
    fields=map;
  }
  return fields;
}
