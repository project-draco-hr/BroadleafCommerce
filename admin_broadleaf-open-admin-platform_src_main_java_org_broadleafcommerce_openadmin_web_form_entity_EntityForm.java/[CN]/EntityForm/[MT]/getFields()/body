{
  if (fields == null) {
    Map<String,Field> map=new LinkedHashMap<String,Field>();
    for (    Entry<String,Tab> entry : tabs.entrySet()) {
      for (      FieldGroup group : entry.getValue().getFieldGroups()) {
        for (        Field field : group.getFields()) {
          map.put(field.getName(),field);
        }
      }
    }
    fields=map;
  }
  return fields;
}
