{
  if (fieldGroups == null) {
    Map<String,FieldGroup> map=new LinkedHashMap<String,FieldGroup>();
    for (    Entry<String,Tab> entry : tabs.entrySet()) {
      for (      FieldGroup fieldGroup : entry.getValue().getFieldGroups()) {
        map.put(fieldGroup.getTitle(),fieldGroup);
      }
    }
    fieldGroups=map;
  }
  return fieldGroups;
}
