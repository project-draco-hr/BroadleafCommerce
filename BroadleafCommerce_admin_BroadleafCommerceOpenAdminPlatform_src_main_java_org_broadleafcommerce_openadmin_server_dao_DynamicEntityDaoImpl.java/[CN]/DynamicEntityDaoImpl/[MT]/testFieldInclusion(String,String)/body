{
  Boolean includeField=true;
  if (!StringUtils.isEmpty(prefix) && prefix.contains(propertyName)) {
    int start=0;
    int count=0;
    boolean eof=false;
    while (!eof) {
      int pos=prefix.indexOf(propertyName,start);
      if (pos >= 0) {
        start+=pos + propertyName.length();
        count++;
        if (count > 1) {
          includeField=false;
          eof=true;
        }
      }
 else {
        eof=true;
      }
    }
  }
  return includeField;
}
