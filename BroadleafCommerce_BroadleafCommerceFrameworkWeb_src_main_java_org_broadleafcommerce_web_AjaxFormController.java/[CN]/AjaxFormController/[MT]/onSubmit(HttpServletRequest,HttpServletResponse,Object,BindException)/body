{
  Map<String,Object> map=new HashMap<String,Object>();
  String view;
  if (((AjaxFormCommandObject)command).isAjaxRequest()) {
    populateAjax(map,command);
    view=getAjaxView();
  }
 else {
    populateStandard(map,command);
    view=getSuccessView();
  }
  return new ModelAndView(view,map);
}
