{
  if (getClass().isAssignableFrom(o.getClass())) {
    OfferAuditImpl that=(OfferAuditImpl)o;
    return new EqualsBuilder().append(this.id,that.id).append(this.customerId,that.customerId).append(this.offerId,that.offerId).append(this.offerCodeId,that.offerCodeId).append(this.redeemedDate,that.redeemedDate).append(this.orderId,that.orderId).build();
  }
  return false;
}
