{
  return ((getAdditionalFields() != null && !getAdditionalFields().isEmpty()) || getCreditCardHolderName() != null || getCreditCardType() != null || getCreditCardNum() != null || getCreditCardLastFour() != null || getCreditCardExpDate() != null || getCreditCardExpMonth() != null || getCreditCardExpYear() != null || getCreditCardCvv() != null);
