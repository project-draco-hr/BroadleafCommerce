{
  DynamicEntityFormInfo info=new DynamicEntityFormInfo().withCeilingClassName(StructuredContentType.class.getName()).withCriteriaName("constructForm").withPropertyName("structuredContentType");
  entityForm.putDynamicFormInfo("structuredContentType",info);
  return super.saveEntity(request,response,model,SECTION_KEY,id,entityForm,result,ra);
}
