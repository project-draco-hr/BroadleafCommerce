{
  if (sessionFactory == null && beanManager == null)   throw new IllegalArgumentException("You must provide either a SessionFactory or a PersistentBeanManager");
  if (usingProxyClassLoader)   tmpClassloader=new ThreadLocal<ClassLoader>();
  if (beanManager == null)   beanManager=GwtConfigurationHelper.initGwtStatelessBeanManager(new HibernateUtil(sessionFactory));
  super.afterPropertiesSet();
}
