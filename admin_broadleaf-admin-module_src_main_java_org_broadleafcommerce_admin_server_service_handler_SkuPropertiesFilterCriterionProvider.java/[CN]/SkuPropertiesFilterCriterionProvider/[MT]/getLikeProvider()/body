{
  return new SimpleFilterCriterionProvider(FilterDataStrategy.DIRECT,1){
    @Override public Criterion getCriterion(    String targetPropertyName,    Object[] filterObjectValues,    Object[] directValues){
      return buildCriterion(targetPropertyName,Restrictions.ilike(targetPropertyName,(String)directValues[0],MatchMode.START),Restrictions.ilike(skuPropertyPrefix + DEFAULT_SKU_PATH_PREFIX + targetPropertyName,(String)directValues[0],MatchMode.START));
    }
  }
;
}
