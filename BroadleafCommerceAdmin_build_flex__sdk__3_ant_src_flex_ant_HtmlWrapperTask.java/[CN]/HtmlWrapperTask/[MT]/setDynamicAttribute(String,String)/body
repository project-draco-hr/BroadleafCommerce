{
  if (name.equals("version-major")) {
    versionMajor=value;
  }
 else   if (name.equals("version-minor")) {
    versionMinor=value;
  }
 else   if (name.equals("version-revision")) {
    versionRevision=value;
  }
 else {
    throw new BuildException("The <html-wrapper> task doesn't support the \"" + name + "\" attribute.",getLocation());
  }
}
