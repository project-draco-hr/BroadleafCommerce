{
  final int maxSize=getExpressionCacheMaxSize();
  if (maxSize == 0) {
    return null;
  }
  return new StandardCache<String,Object>(getExpressionCacheName(),getExpressionCacheUseSoftReferences(),getExpressionCacheInitialSize(),maxSize,getExpressionCacheValidityChecker(),getExpressionCacheLogger());
}
