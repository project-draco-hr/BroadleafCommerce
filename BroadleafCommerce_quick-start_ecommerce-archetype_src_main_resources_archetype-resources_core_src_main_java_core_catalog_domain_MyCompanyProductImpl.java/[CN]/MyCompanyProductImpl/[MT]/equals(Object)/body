{
  if (this == obj)   return true;
  if (!super.equals(obj))   return false;
  if (getClass() != obj.getClass())   return false;
  MyCompanyProductImpl other=(MyCompanyProductImpl)obj;
  if (restricted != other.restricted)   return false;
  if (shippingCountries == null) {
    if (other.shippingCountries != null)     return false;
  }
 else   if (!shippingCountries.equals(other.shippingCountries))   return false;
  return true;
}
