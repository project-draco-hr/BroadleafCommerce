{
  try {
    Properties props=loadProperties();
    setHandlers(props);
  }
 catch (  IOException e) {
    throw new MergeManagerSetupException(e);
  }
catch (  ClassNotFoundException e) {
    throw new MergeManagerSetupException(e);
  }
catch (  IllegalAccessException e) {
    throw new MergeManagerSetupException(e);
  }
catch (  InstantiationException e) {
    throw new MergeManagerSetupException(e);
  }
}
