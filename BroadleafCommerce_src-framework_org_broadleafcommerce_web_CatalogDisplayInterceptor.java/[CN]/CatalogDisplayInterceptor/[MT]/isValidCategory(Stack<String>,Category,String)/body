{
  if (tokens.isEmpty()) {
    return true;
  }
  String token=tokens.pop();
  boolean check;
  if (category != null && token.equals(category.getUrlKey())) {
    check=isValidCategory(tokens,category.getDefaultParentCategory(),ignoreToken);
  }
 else {
    if (category == null && token.equals(ignoreToken)) {
      return true;
    }
 else {
      return false;
    }
  }
  return check;
}
