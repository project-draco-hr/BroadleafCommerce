{
  String cookieData=null;
  try {
    Cookie[] cookies=request.getCookies();
    if (cookies != null) {
      for (      Cookie cookie : cookies) {
        if (cookie.getName().equals(COOKIE_NAME)) {
          cookieData=cookie.getValue();
          break;
        }
      }
    }
  }
 catch (  Exception e) {
  }
  return cookieData;
}
