{
  for (int i=(order.getOrderItems().size() - 1); i > 0; i--) {
    OrderItem currentItem=(order.getOrderItems().get(i));
    if (currentItem instanceof BundleOrderItem) {
      if (bundleItemMatches((BundleOrderItem)currentItem,itemToFind)) {
        return currentItem;
      }
    }
  }
  return null;
}
