{
  if (sku.getSkuAttributes() == null || sku.getSkuAttributes().size() == 0) {
    for (    SkuAttribute skuAttribute : sku.getSkuAttributes()) {
      if (attributeName.equals(skuAttribute.getName()) && attributeValue.equals(skuAttribute.getValue())) {
        return true;
      }
    }
  }
  return false;
}
