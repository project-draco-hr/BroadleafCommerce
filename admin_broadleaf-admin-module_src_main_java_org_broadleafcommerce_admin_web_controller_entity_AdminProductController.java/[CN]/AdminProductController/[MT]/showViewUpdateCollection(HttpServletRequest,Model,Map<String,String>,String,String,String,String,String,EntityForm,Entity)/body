{
  try {
    if ("additionalSkus".equals(collectionField)) {
      return showUpdateAdditionalSku(request,model,id,collectionItemId,pathVars,entityForm);
    }
 else {
      return super.showViewUpdateCollection(request,model,pathVars,id,collectionField,collectionItemId,alternateId,modalHeaderType,entityForm,entity);
    }
  }
 catch (  Exception e) {
    throw new ServiceException(e);
  }
}
