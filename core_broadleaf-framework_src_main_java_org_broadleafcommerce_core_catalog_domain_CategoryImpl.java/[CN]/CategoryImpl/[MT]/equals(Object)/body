{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  CategoryImpl other=(CategoryImpl)obj;
  if (id != null && other.id != null) {
    return id.equals(other.id);
  }
  if (name == null) {
    if (other.name != null) {
      return false;
    }
  }
 else   if (!name.equals(other.name)) {
    return false;
  }
  if (url == null) {
    if (other.url != null) {
      return false;
    }
  }
 else   if (!url.equals(other.url)) {
    return false;
  }
  return true;
}
