{
  FormItem displayFormItem=null;
switch (SupportedFieldType.valueOf(fieldType)) {
case FOREIGN_KEY:
    displayFormItem=new HiddenItem();
  displayFormItem.setName("__display_" + field.getName());
break;
case ADDITIONAL_FOREIGN_KEY:
displayFormItem=new HiddenItem();
displayFormItem.setName("__display_" + field.getName());
break;
}
return displayFormItem;
}
