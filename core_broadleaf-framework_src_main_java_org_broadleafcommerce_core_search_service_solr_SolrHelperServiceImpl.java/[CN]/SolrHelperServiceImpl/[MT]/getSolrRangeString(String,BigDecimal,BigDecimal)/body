{
  StringBuilder sb=new StringBuilder();
  sb.append(fieldName).append(":[");
  if (minValue == null) {
    sb.append("*");
  }
 else {
    sb.append(minValue.toPlainString());
  }
  sb.append(" TO ");
  if (maxValue == null) {
    sb.append("*");
  }
 else {
    sb.append(maxValue.toPlainString());
  }
  sb.append(']');
  return sb.toString();
}
