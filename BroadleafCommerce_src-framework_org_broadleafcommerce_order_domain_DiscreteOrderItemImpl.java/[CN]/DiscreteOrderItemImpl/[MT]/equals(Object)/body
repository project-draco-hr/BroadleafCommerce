{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  DiscreteOrderItemImpl other=(DiscreteOrderItemImpl)obj;
  if (id != null && other.id != null) {
    return id.equals(other.id);
  }
  if (bundleOrderItem == null) {
    if (other.bundleOrderItem != null)     return false;
  }
 else   if (!bundleOrderItem.equals(other.bundleOrderItem))   return false;
  if (sku == null) {
    if (other.sku != null)     return false;
  }
 else   if (!sku.equals(other.sku))   return false;
  return true;
}
